<mapper namespace="com.yc.xmj.mapper.AdminMapper">
		
	<select id="getAdmin" parameterType="Admin" resultType="Admin">
	    select * from admin where a_name=#{a_name} and  a_password = #{a_password}
	</select>
</mapper>